{
  "compilerOptions": {
    "target": "ES6",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    // "sourceMap": true,
  },
  "include": ["src/**/*.ts", "server.ts"],
  "exclude": ["tailwind.config.js", "**/*.d.ts"],
}
